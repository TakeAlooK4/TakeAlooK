<body>

<th:block th:include="header.html"/>
	
	<section style="width: 70%; margin: auto;" >
	<h2>반려동물 케어</h2>
	
	
	<!-- 아코디언시작 -->
	<div id="accordion" >
	
		<!-- for each문 시작 -->

		<th:block th:each="p,index:${pet}">
		<div class="petGroup"  >
		
			<!-- 제목 -->
			<h3 th:text="|${p.petName} ${#dates.format(p.petBirth,'yyyy-MM-dd')}|"  th:id="${p.petId}" ></h3>
			
			<!-- 반려동물정보 -->
			<div align="center">

				<!--  하나의 동물정보 -->			
				<table class="tableSpace" >
				
					<tr valign="top" >
						
						<!-- [ 정보수정 ] -->
						<td colspan="2">
							
							<button type="button" id="petDelete" th:value="${p.petId}" style="float: right">삭제</button>
							
							<button th:id="|updateOpener${index.index}|" class="opener" th:value="${p.petId}" style="float:right;">정보 수정</button>
							
							
							<div th:id="|petUpdate${index.index}|" class="openModal" title="정보 수정">
					
								<form action="/petUpdate" method="post" >
								 	<input type="text" name="petId" th:value="${p.petId}" id="id" >
								 	
								 	<input type="text" name="nickname" th:value="${p.nickname.nickname}" hidden="true">
									<label for="petImg">반려동물 사진 : [사진을  드래그 하시오.]</label>
									<!-- <input type="text" name="petImg" text="${p.petImg}" hidden="true"> -->
									<div class="fileDrop"><div class="uploadedList"><img height="150" width="200" th:src="|/upload/displayFile?fileName=${p.petImg}|"/></div></div>
									<input type="file">
									<br/>
								 
								 	<label for="petName">반려동물 이름 : </label>
								 	<input type="text" th:id="petName" th:value="${p.petName}" name="petName" required="required">
								 	<br/>
								 	
								 	<!-- 라디오 버튼 -->
								 	<div th:switch="${p.petCode}">
								 	<label for="petCode">반려동물 유형 : </label>
									 	<span th:case="D">
									 		<input type="radio" name="petCode" th:value="D" checked="checked" required="required">강아지
									 		<input type="radio" name="petCode" th:value="C" required="required">고양이
									 	</span>
									 	<span th:case="C">
									 		<input type="radio" name="petCode" th:value="D" required="required">강아지
									 		<input type="radio" name="petCode" th:value="C" checked="checked" required="required">고양이 
								 		</span>
								 	</div>
				
								 	<label for="breedCode">반려동물 품종 : </label>
									<select class="breedCode" name="breedCode" required="required">
										<option th:value="${p.breedCode}" th:text="${p.breedCode.breedName}" selected="selected"></option>
										<option th:each="b:${breed}" th:value="${b.breedCode}" th:text="${b.breedName}"></option>
									</select>
								 	<br/>
								 	
								 	<div th:switch="${p.petGender}">
								 	 <label for="petGender">반려동물 성별 : </label>
									 	<span th:case="M">
									 		<input type="radio" name="petGender" th:value="M" checked="checked" required="required">수컷
								 			<input type="radio" name="petGender" th:value="F" required="required">암컷
									 	</span>
									 	<span th:case="F">
									 		<input type="radio" name="petGender" th:value="M" required="required">수컷
								 			<input type="radio" name="petGender" th:value="F" checked="checked" required="required">암컷
								 		</span>
								 	</div>
				
									<label for="petBirth">반려동물 생일 : </label>
								 	<input type="text" class="datepicker" th:value="${#dates.format(p.petBirth,'yyyy-MM-dd')}" name="petBirth" required="required">									 	<br/>
								 		
								 	<input type="submit" value="수정" >
							 	</form>
						 	
						</div><!-- 정보수정끝 -->
							
						</td><!-- 정보수정끝  -->
						
					</tr>
					
					<!--[사진 / 정보]-->
					<tr valign="middle" >
					
						<!-- 사진영역 -->
						<td>
							<table> 
								<tr>
									<td><img height="150" width="200" th:src="|/upload/displayFile?fileName=/${p.petImg}|"/></td>
								</tr>
							</table>
						</td>
						
						<!-- 기본 정보 영역 -->
						<td>
							<table height="150" width="500" border="1"> 
								<tr>
									<th>유형</th>
									<td th:text="${p.petCode}=='D'? '강아지':'고양이'"></td>	
									
									<th>품종</th>
									<td th:text="${p.breedCode.breedName}"></td>
								</tr>
												
								<tr>
									<th>나이</th>
									<td th:text="|(만) ${p.petAge} 세|"></td>
									
									<th>생일</th>
									<td th:text="${#dates.format(p.petBirth,'yyyy-MM-dd')}" />
								</tr>
											
								<tr>
									<th>의료기록</th>
									<td class="money" th:text="|${count}(건) / ${total}(원)|"></td>
									
									<th>접종(검사)예정일</th>
									<td class="petCycle"> <span style="color: red;" th:text="|${tCycle[0].lifeCycleNo.tCode.tName} (${tCycle[0].clinicDate})|"></span></td>
								</tr>
							</table>
						</td>
					</tr>	
						
					<!-- [생애주기 차트 / 의료기록] -->
					<tr valign="bottom">
					
						<!-- 생애주기 -->
						<td>
							<table class="petTCycle">
								
								<tr >
									<td>
									<span th:text="|${tCycle[1].lifeCycleNo.tCode.tName} (${tCycle[1].clinicDate})|"></span><br><br>
									<span th:text="|${tCycle[0].lifeCycleNo.tCode.tName} (${tCycle[0].clinicDate})|" style="color: red;" ></span><br><br>
									<span th:text="|${tCycle[2].lifeCycleNo.tCode.tName} (${tCycle[2].clinicDate})|"></span>
									</td>
								</tr>
								
							</table>
						</td><!--둘째줄 왼쪽 끝 테이블-->
						
						
						<!-- 의료기록 -->
						<td>
							<table border="1" width="500">
							
								<thead>
									<tr>
										<th align="right" colspan="4">
									
										<button th:id="|medicalOpener${index.index}|" class="opener"> + </button>
											<div th:id="|medicalInsert${index.index}|" class="openModal" title="의료 기록" hidden="true">
											  <!-- 의료기록 등록 --> 	
 	
												<form action="/medicalInsert"  method="post" id="medicalForm" name="medicalInsert">
																							 	
													<input type="text" name="petId" th:value="${p.petId}" > 
														
												 	<label>병원이름 : </label><br>
												 	<input type="text" name="clinicHos" required="required">
												 	<br/>
												 	
												 	<label>진단명 : </label><br>
												 	<!-- 대분류 --> 
												 	<select style="width:180px" class="fClinic" required="required">
												 		<option value="" selected="selected">--대분류--</option>
												 		<option th:each="f:${fClinic}"  th:text="${f.fName}" th:value="${f.fCode}" ></option>
												 	</select>
												 	
												 	<!--중분류-->
												 	<select style="width:180px" class="sClinic" required="required">
												 		<option>--중분류--</option>
												 		<option th:each="s:${sClinic}"  th:text="${s.sName}" th:value="${s.sCode}" ></option>
												 	</select>
												 	
												 	<!--소분류-->
												 	<select style="width:180px" name="tCode" class="tClinic" required="required">
												 		<option value="" selected="selected">--소분류--</option>
												 		<option th:each="t:${tClinic}"  th:text="${t.tName}" th:value="${t.tCode}" ></option>
												 	</select> 
												 	
												 	<!-- <input type="text" name="clinicName" hidden="true"> -->
												 	<br/>
												 	
												 	
												 	<label>의료일 : </label><br>
												 	<input type="text" name="medicalDate" class="datepicker" required="required">
												 	<br/>
												 	
												 	<label>의료비 : </label><br>
												 	<input type="text" name="medicalCost" required="required">
												 	<br/>
												 	
												 	<label>메모 : </label><br>
												 	<textarea rows="15" cols="65" name="medicalMemo" style="resize: none;"></textarea>
												 	<br/>
												 	
												 	
												 	<input type="submit" value="등록" >
														
												</form>
											 	
						 					</div><!-- 정보수정끝 -->
										</th>
									</tr>
									<tr>
										<th>병원이름</th>
										<th>진단명</th>
										<th>의료일</th>
										<th>의료비(원)</th>
										
									</tr>
								</thead>
								<tbody align="center" class="medicalList">
									
									<th:block th:each="m,num:${medical}">
									<tr >
										<td th:text="${m.clinicHos}"></td>
										<td><a href="javascript:void(0);" class="mSelectOpner" th:id="${m.medicalNo}"><span th:text="${m.tCode.tName}"></span></a></td>
										<!-- <td><button class="mSelectOpner" th:id="${m.medicalNo}"><span th:text="${m.tCode.tName}"></span></button></td> -->
										<td th:text="${#dates.format(m.medicalDate,'yyyy-MM-dd')}"></td>
										<td th:text="${#numbers.formatInteger(m.medicalCost, 3, 'COMMA')}"></td>
									</tr>
									
									
									
									<!-- 의료기록 수정 -->
										<div th:id="|medicalUpdate${m.medicalNo}|" class="openModal" title="의료기록 " hidden="true" >
											<form action="/medicalUpdate" method="post" >
												<!-- <input type="text" th:value="${m.petId}" hidden="true" name="petId"> -->
												<input type="text" th:value="${m.medicalNo}" hidden="true" name="medicalNo" >
											 	<label>병원이름 : </label><br>
											 	<input type="text" name="clinicHos" th:value="${m.clinicHos}" required="required">
											 	<br/>
											 	
											 	<label>진단명 : </label><br>
											 	<!-- 대분류 --> 
											 	<select style="width:180px" class="fClinic" required="required">
											 		<option th:value="${m.tCode.sCode.fCode}" th:text="${m.tCode.sCode.fCode.fName}" ></option>
											 		<option th:each="f:${fClinic}"  th:text="${f.fName}" th:value="${f.fCode}" ></option>
											 	</select>
											 	
											 	<!--중분류-->
											 	<select style="width:180px" class="sClinic" required="required">
											 		<option th:value="${m.tCode.sCode}" th:text="${m.tCode.sCode.sName}" ></option>
											 		<option th:each="s:${sClinic}"  th:text="${s.sName}" th:value="${s.sCode}" ></option>
											 	</select>
											 	
											 	<!--소분류-->
											 	<select style="width:180px" name="tCode" class="tClinic" required="required">
											 		<option th:value="${m.tCode}" th:text="${m.tCode.tName}" selected="selected"></option>
											 		<option th:each="t:${tClinic}"  th:text="${t.tName}" th:value="${t.tCode}" ></option>
											 	</select> 
											 	
											 	<input type="text" name="clinicName" hidden="true">
											 	<br/>
											 	
											 	
											 	<label>의료일 : </label><br>
											 	<input type="text" name="medicalDate" class="datepicker" th:value="${m.medicalDate}" required="required">
											 	<br/>
											 	
											 	<label>의료비 : </label><br>
											 	<input type="text" name="medicalCost" th:value="${m.medicalCost}" required="required">
											 	<br/>
											 	
											 	<label>메모 : </label><br>
											 	<textarea rows="15" cols="65" name="medicalMemo" style="resize: none;" th:text="${m.medicalMemo}">
											 	
											 	</textarea>
											 	<br/>   
											 	
											 	
												<button type="button" id="medicalDelete" style="float: right" th:value="${m.medicalNo}">삭제</button>	
											 	<input type="submit" value="수정" style="float: right">
												</form>
																					
										</div>
										
									</th:block>
									
								</tbody>
							</table>
						</td><!--둘째줄 오른쪽 끝 테이블-->
					</tr><!--둘째줄 tr끝-->
						
				
				</table><!--전체 윤곽 테이블 끝-->
				
			</div><!-- 반려동물정보 -->
		</div><!-- for each문-->
		</th:block>
	</div><!-- 아코디언 -->

	<button id="insertOpener" class="InsertOpener">정보 등록</button>
	
	<!-- 정보등 -->
 	 <div id="petInsert" class="openModal" title="반려동물 등록">
 	
	 	<form action="/petInsert" method="post" name="petInsert">
	 		
	 		<input type="text" th:value="${user.nickname}" hidden="true" name="nickname">
	 		
		 	<label for="petImg">반려동물 사진 : [사진을  드래그 하시오.] </label> 
		 	<div class="fileDrop"><div class="uploadedList" ></div></div>
		 	
		 	<br/>
		 	
		 	<label for="petName">반려동물 이름 : </label>
		 	<input type="text" name="petName" required="required"><br/>
		
		 	<label for="petCode">반려동물 유형 : </label>
		 	<input type="radio" name="petCode" value="D" required="required">강아지
		 	<input type="radio" name="petCode" value="C" required="required">고양이
		 	<br/>
		 	
		 	<label for="breedCode">반려동물 품종 : </label>
			<select class="breedCode" name="breedCode" required="required">
					<option value="" selected="selected">--품종 선택--</option>
					<option th:each="b:${breed}" th:value="${b.breedCode}" th:text="${b.breedName}"></option>
			</select>
		 	<br/>
		 	
		 	<label for="petCode">반려동물 성별 : </label>
		 	<input type="radio" name="petGender" th:value="M" required="required">수컷
		 	<input type="radio" name="petGender" th:value="F" required="required">암컷
		 	<br/>
		 	
		 	<label for="datepicker">반려동물 생일 : </label>
		 	<input type="text" class="datepicker" name="petBirth" required="required">
		 	<br/>
		 	
		 	<input type="submit" value="등록" >
	 	</form>
 	</div>
 	
 	<br/>
 </section>

 	
