/*footer*/
footer{margin-top: 100px;}
/*common*/
.wrapper{width: 1170px; margin: 0 auto; padding: 0;}
.cleanfix{content: ''; display: block; clear: both;}

/*reset*/
*{margin: 0; padding: 0;}
li{list-style: none;}
a{text-decoration: none;}
