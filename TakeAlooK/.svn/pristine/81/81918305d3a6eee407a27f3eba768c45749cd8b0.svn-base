@charset "utf-8";

/*body*/

/*header*/
header{height: 75px; background-color: #F8F8FF; border: 1px solid #F8F8FF;
		position: fixed; width: 100%; z-index: 9999; top: 0; left: 0;}
h1{color: graytext; line-height: 65px; float: left; left: auto;}
.menu{float: right;}
.menu li {float: left; line-height: 65px; color: graytext; padding: 0 5px;}
